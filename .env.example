# WhatsApp Monitor Configuration

# Server port (default: 3000)
PORT=3000

# Webhook URL for external notifications (optional)
# Example: https://your-server.com/whatsapp-webhook
WEBHOOK_URL=

# Database file path (default: ./whatsapp_data.db)
DB_FILE=./whatsapp_data.db

# Authentication directory (default: ./auth_info)
AUTH_DIR=./auth_info

# Logging level (default: warn)
LOG_LEVEL=warn